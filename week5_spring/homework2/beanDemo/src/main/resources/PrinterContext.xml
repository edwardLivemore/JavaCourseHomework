<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="printerA" class="com.example.demo.pojo.Printer" init-method="initMethod" destroy-method="destroyMethod">
        <property name="id" value="1"/>
        <property name="name" value="greatPrinter"/>
    </bean>

    <bean id="printerB" class="com.example.demo.pojo.Printer" init-method="initMethod" destroy-method="destroyMethod">
        <property name="id" value="2"/>
        <property name="name" value="normalPrinter"/>
    </bean>

    <context:component-scan base-package="com.example.demo.processor"/>
</beans>